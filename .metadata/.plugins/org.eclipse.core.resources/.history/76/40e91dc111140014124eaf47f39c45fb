package de.rjr910.audio;

import de.rjr910.games.WordGame;

public class AudioStoppedHandler implements Runnable{

	private CallBackStopped wg;
	
	public AudioStoppedHandler(WordGame wg) {

		this.wg = wg;
	}

	@Override
	public void run() {
		System.out.println("stopped");
		wg.generateAnswers();
		
	}

}
